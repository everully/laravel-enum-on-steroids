{
    "name": "everully/laravel-enum-on-steroids",
    "description": "This package provides Enum trait to speed up the development of Laravel applications.",
    "keywords" : [
        "everully",
        "laravel",
        "laravel-on-steroids",
        "enums",
        "php",
        "Laravel 9",
        "Laravel 10",
        "Laravel 11",
        "Laravel 12"
    ],
    "homepage" : "https://github.com/everully/everully/laravel-enum-on-steroids",
    "license" : "MIT",
    "autoload": {
        "psr-4": {
            "Everully\\LaravelEnumOnSteroids\\": "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "Everully\\LaravelEnumOnSteroids\\Tests\\" : "tests/"
        }
    },
    "authors": [
        {
            "name": "everully",
            "role" : "Developer"
        }
    ],
    "require" : {
        "php": "^8.1",
        "illuminate/contracts": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "laravel/pint": "^1.17"
    },
    "scripts" : {
        "test" : "./vendor/bin/pest --no-coverage",
        "test-coverage" : "vendor/bin/pest --coverage-html coverage",
        "pint" : "/vendor/bin/pint"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability" : "dev",
    "prefer-stable" : true
}
